{% for item in items %}
    {% if item.active %}
        <span class="list-item active" title="{{ 'Current page'|t }}">
    {% elseif item.url == false %}
        <span class="list-item unavailable">
    {% else %}
        <span class="list-item">
    {% endif %}
        <span class="list-item__avatar">
            {% if item.type == "video" %}
                <svg class="course-material-navigation__icon" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path d="M0 0h24v24H0z" fill="none"/><path d="M17 10.5V7c0-.55-.45-1-1-1H4c-.55 0-1 .45-1 1v10c0 .55.45 1 1 1h12c.55 0 1-.45 1-1v-3.5l4 4v-11l-4 4z"/></svg>
            {% elseif item.type == "article" %}
                <svg class="course-material-navigation__icon" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path d="M14 17H4v2h10v-2zm6-8H4v2h16V9zM4 15h16v-2H4v2zM4 5v2h16V5H4z"/><path d="M0 0h24v24H0z" fill="none"/></svg>
            {% endif %}
        </span>
        <span class="list-item__text">
            {% if item.url %}
                <a href="{{ item.url }}">
            {% endif %}
            {% trans %}Part {{ item.number }} - {{ item.label }}{% endtrans %}
            {% if item.url %}
                </a>
            {% endif %}
        </span>
    </span>
{% endfor %}
